{
  "connection": "default",
  "collectionName": "Blog Post",
  "info": {
    "name": "blogpost",
    "description": "Blog Post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "text"
    },
    "user": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "date": {
      "type": "date"
    },
    "slug": {
      "type": "string"
    }
  }
}